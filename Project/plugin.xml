        name="%Report">
  </page>
  <page
        category="project.preferences.UCM2DEVS"
        class="project.preferences.SystemConfigurationPreferencePage"
        id="project.preferences.SystemConfigurationPreferencePage"
        name="%SystemConfiguration">
  </page>	
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
