  <page
        category="project.preferences.UCM2DEVS"
        class="project.preferences.SoftwareArchitectureEvaluationPreferencePage"
        id="project.preferences.SoftwareArchitectureEvaluationPreferencePage"
        name="%Evaluation">
  </page>
  <page
        category="project.preferences.UCM2DEVS"
        class="project.preferences.ReportsPreferencePage"
