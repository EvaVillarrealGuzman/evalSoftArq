       		id="project.preferences.EditQualityRequirementPreferencePage"
       		name="%EditQualityRequirement">
	 </page>	
	 <page
       		category="project.preferences.UCM2DEVS"
       		class="project.preferences.EditSystemPreferencePage"
       		id="project.preferences.EditSystemPreferencePage"
       		name="%EditSystem">
	 </page>
  <page
